{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "description": "weekly calendar for antd",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoavweber/antd-weekly-calendar.git"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "antd-weekly-calendar",
  "author": "yoavweber",
  "module": "dist/antd-weekly-calendar.esm.js",
  "size-limit": [
    {
      "path": "dist/antd-weekly-calendar.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/antd-weekly-calendar.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "husky": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5",
    "antd": "^4.16.12",
    "date-fns": "^2.23.0"
  }
}
